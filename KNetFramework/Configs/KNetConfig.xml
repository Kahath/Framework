<?xml version="1.0" encoding="utf-8" ?>
<KNetSettings>
	<!--  Server settings -->
	<property name="bindip" value="0.0.0.0"/>
	<!--Server bind IP-->
	<property name="bindport" value=""/>
	<!--Server port-->

	<!-- Database settings-->
	<property name="dbhost" value="127.0.0.1"/>
	<!--Database IP-->
	<property name="dbport" value="1433"/>
	<!--Database port-->
	<property name="dbuser" value="root"/>
	<!--Database username-->
	<property name="dbpass" value="root"/>
	<!--Database password-->
	<property name="dbname" value=""/>
	<!--Database name-->

	<!-- Log level settings-->
	<!--Console log level mask
		None				= 0x00
		Normal				= 0x01
		Init				= 0x02
		Command				= 0x04
		DB					= 0x08
		Info				= 0x10
		Warning				= 0x20
		Error				= 0x40
		Critical			= 0x80
		All					= 0xFF-->
	<property name="loglevel" value="0xFF" />

	<!--Packet log level settings-->
	<!--Packet log level mask
		None				= 0x00
		SMSG				= 0x01
		CMSG				= 0x02
		All					= 0x03-->
	<property name="packetloglevel" value="0x00" />
	<property name="packetlogsize" value="1000" />

	<!--Opcode settings-->
	<!--Opcode allow type mask
		NotUsed				= 0x01
		InDevelopment		= 0x02
		Test				= 0x04
		Stable				= 0x08
		Release				= 0x10
		All					= 0x1F-->
	<property name="opcodeallowlevel" value="0x1F"/>

	<!-- Socket settings-->
	<property name="maxconnections" value="10" />
	<property name="buffersize" value="4096" />
	<property name="maxsimultaneousacceptops" value="10" />
	<property name="backlog" value="100" />
	<property name="assemblypath" value="Assemblies" />
	<property name="logfolderpath" value="Logs" />

	<!--Endianness type
		LittleEndian		= 0x01
		BigEndian			= 0x02-->
	<property name="endianness" value="0x01" />
</KNetSettings>